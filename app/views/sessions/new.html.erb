<h4>Login</h4>

<%= render 'shared/flashes' %>

<div class='well'>
  <%= form_tag '/sessions' do %>
    <div class='control-group'>
      <%= label_tag :username %>
    </div>
    <div>
      <%= text_field_tag :username %>
    </div>
    <div>
      <%= label_tag :password %>
    </div>
    <div>
      <%= password_field_tag :password %>
    </div>
    <div>
      <%= submit_tag 'Submit', class:'btn btn-success' %>
    </div>
  <% end %>
</div>

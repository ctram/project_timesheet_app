<h4>Login</h4>
<div class='well well-sm'>
  <%= form_for @user do |f| %>
    <div class='control-group'>
      <%= f.label :username %>
      <%= f.text_field :username %>
    </div>
    <div class='control-group'>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <div class='control-group'>
      <%= f.submit 'Submit', class:'btn btn-success' %>
    </div>


  <% end %>
</div>
